{
  "name": "ganaa_backend",
  "version": "1.0.0",
  "description": "Ganna Backend Server",
  "scripts": {
    "prestart": "npm run build",
    "start": "node build/server.js",
    "seed": "NODE_OPTIONS='--max-old-space-size=4096' nodemon add_dummy_data.ts",
    "start:dev": "nodemon src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.json src/server.ts",
    "build": "rimraf ./build && tsup src/server.ts --format cjs --out-dir build --no-dts --minify --clean",
    "start:tsc": "node build/src/server.js",
    "build:tsc": "rimraf ./build && tsc",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "gadgetvala",
  "license": "ISC",
  "engines": {
    "npm": ">=10.9.0",
    "node": ">=22.12.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/connect-timeout": "^0.0.39",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/validator": "^13.12.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/s3-request-presigner": "^3.782.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node-cron": "^3.0.11",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "validator": "^13.15.0",
    "xss": "^1.0.15"
  }
}
